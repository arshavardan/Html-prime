{
  "name": "server",
  "version": "0.0.1",
  "description": "Config Microservice is a set of API's for managing configuration of DCManager",
  "main": "app/index.ts",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "yarn rollup -c -w",
    "build": "NODE_ENV=production yarn rollup -c",
    "docs": "yarn apidoc -i app/routes -o dist/doc -t .apidoc/",
    "lint": "yarn eslint ./app --ext .ts",
    "test": "yarn vitest",
    "test:ui": "yarn vitest --ui",
    "test:coverage": "yarn vitest --coverage",
    "security:audit": "yarn audit --groups dependencies"
  },
  "apidoc": {
    "name": "Config Microservice"
  },
  "dependencies": {
    "axios": "^1.3.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "http-status": "^1.6.2",
    "ioredis": "^5.3.1",
    "joi": "^17.8.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "pino": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-run": "^3.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.44",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitest/coverage-istanbul": "^0.29.1",
    "@vitest/ui": "^0.29.1",
    "apidoc": "^0.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-inject-string": "^1.1.2",
    "gulp-rename": "^2.0.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-template": "^5.0.0",
    "husky": "^8.0.3",
    "minimist": "^1.2.8",
    "pluralize": "^8.0.0",
    "prettier": "^2.8.4",
    "rollup": "^3.17.3",
    "rollup-plugin-copy": "^3.4.0",
    "supertest": "^6.3.3",
    "typescript": "^4.9.5",
    "unplugin-swc": "^1.3.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  },
  "resolutions": {
    "minimist": "1.2.8",
    "moment": "^2.29.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
